<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_fetch_data" model="ir.ui.view">
            <field name="name">fetch.data</field>
            <field name="model">fetch.data</field>
            <field name="arch" type="xml">
                <form string="Fetch Data">
                    <group>
                        <group>
                            <separator string="Keyword"/>
                            <field name="keyword" nolabel="1"/>
<!--                            <field name="page_size"/>-->
<!--                            <field name="page_no"/>-->
                        </group>
                        <group>
                        </group>
                    </group>
                    <footer>
                        <button icon="fa-cogs" string="Fetch Data" name="fetch_data" type="object" class="oe_highlight"/>
                        <button icon="fa-cogs" string="Fetch Products" name="fetch_products_data" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
               </form>
            </field>
        </record>

        <record id="action_view_fetch_data" model="ir.actions.act_window">
            <field name="name">Fetch Data</field>
            <field name="res_model">fetch.data</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_fetch_data"/>
            <field name="target">new</field>
        </record>

         <menuitem id="menu_integration_distributor_wizard" name="Fetch Catalog"
                  action="action_view_fetch_data"
                  parent="menu_integration" sequence="2"/>

</odoo>